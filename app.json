{
  "expo": {
    "name": "synctimes-rn",
    "slug": "synctimes-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "synctimes",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.cauldnclark.synctimes",
      "buildNumber": 2
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cauldnclark.synctimesrn",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "64144bca-cef5-478a-882d-b9fbb92adfb8"
      }
    },
    "owner": "cauldnclark",
    "updates": {
      "url": "https://u.expo.dev/64144bca-cef5-478a-882d-b9fbb92adfb8"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
